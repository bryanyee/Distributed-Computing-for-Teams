{
  "name": "DeThread",
  "version": "1.0.0",
  "description": "An application library for distributed computing with JavaScript, WebSockets, and web workers in the browser.",
  "main": "index.js",
  "repository": "https://github.com/dethread/dethread",
  "scripts": {
    "start": "node ./server/server.js",
    "startDev": "nodemon ./server/server.js",
    "startDevServer": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --history-api-fallback",
    "test": "mocha --reporter nyan --compilers js:babel-core/register --require ./test/.setup.js --recursive ./test",
    "enzyme": "mocha test/.setup.js test/test.js",
    "test:watch": "npm run test -- --watch",
    "test-routes": "mocha --timeout 20000 tests/",
    "build": "webpack --mode=production",
    "buildDev": "webpack --mode=development"
  },
  "author": "Daniel Lao, Bryan Yee, Shawn Southwell",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@babel/register": "^7.6.2",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-preset-airbnb": "^4.1.0",
    "chai": "^3.5.0",
    "chai-jquery": "^2.0.0",
    "css-loader": "^3.2.0",
    "enzyme": "^2.5.1",
    "eslint": "^6.6.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "jquery": "^2.2.1",
    "jsdom": "^8.1.0",
    "mocha": "^2.4.5",
    "node-sass": "^4.13.0",
    "react-addons-test-utils": "^0.14.7",
    "sass-loader": "^8.0.0",
    "sinon": "^1.17.6",
    "style-loader": "^1.0.0",
    "supertest": "^2.0.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.9.0"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "enzyme": "^2.5.1",
    "express": "^4.14.0",
    "lodash": "^3.10.1",
    "nodemon": "^1.11.0",
    "prop-types": "^15.7.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-router-dom": "^5.1.2",
    "sass": "^0.5.0",
    "socket.io": "^1.5.0"
  }
}
